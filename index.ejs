<html lang="en">

<body class="container">

  
    <main>
        <div >
			  <table>
			  	<tr>
				<th>Name</th>
				<th>Height</th>
				</tr>
				<% persons.forEach(function(person) { %>
				<tr>
				<td><%= person.name %></td>
				<td><%= person.height %></td>
				</tr>
				 <% }); %>
			  </table>   
	</div>
    </main>

    
</body>
</html>